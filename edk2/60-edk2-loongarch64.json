{
    "description": "LoongArch UEFI for loongarch64",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "/usr/share/edk2/loongarch64/QEMU_CODE.fd",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "/usr/share/edk2/loongarch64/QEMU_VARS.fd",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "loongarch64",
            "machines": [
                "virt-*"
            ]
        }
    ],
    "features": [
        "verbose-static"
    ],
    "tags": [

    ]
}
